teidx=kmeans(X(em_idx(29,1):em_idx(29,2)),130/2)
 for i=1:length(teidx)
    plot(X(teidx==i),ones(1,length(X(teidx==i))),'*')
    text(X(teidx==i),ones(1,length(X(teidx==i))),num2str(i));
    hold on
end
